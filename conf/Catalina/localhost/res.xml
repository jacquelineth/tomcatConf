    <Parameter name="ilog.rules.res.XOM_IN_RULEAPP_ARCHIVE" value="true" override="false" />
    <Parameter name="management.tcpip.timeout" value="120" override="false" />

	<!--<Parameter name="JMX_XU_QUERY_PART" value="xuName" override="false" />	-->
</Context>