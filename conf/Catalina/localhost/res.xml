<?xml version='1.0' encoding='utf-8' ?>
<Context  docBase="${executionserver.home.tomcat}/res.war" reloadable="true">
    <Logger className="org.apache.catalina.logger.FileLogger" prefix="odm88_res_log" suffix=".txt" timestamp="true" />
    <Resource name="jdbc/resdatasource" type="javax.sql.DataSource" auth="Container"
     username="resdbAdmin" password="resdbAdmin" 
     driverClassName="org.apache.derby.jdbc.EmbeddedDriver" 
     url="jdbc:derby:../data/resDb88;create=true" />
    <!--Modify the WEB-INF/web.xml to set the modified context path for teamserver, testing, decisionService -->
    <Parameter name="management.protocol" value="tcpip" override="false" />
	<!--<Parameter name="JMX_XU_QUERY_PART" value="xuName" override="false" />	-->
</Context>